#!/bin/bash
#source ~/dotfiles/bashconf.sh
echo "Config files coping..."
cp ~/dotfiles/.bashrc ~/
cp ~/dotfiles/.vimrc ~/
mkdir -p ~/.vim/autoload/
mkdir -p ~/.vim/colors/
cp ~/dotfiles/my.vim ~/.vim/colors
cp ~/dotfiles/Linux/configs/.vim/autoload/plug.vim ~/.vim/autoload
cp ~/dotfiles/.tmux.conf ~/
#.config/i3/config
#bin -r
#vifmrc
#.Xresources
#.xprofile
#.xinputrc
#.xbindkeysrc
#
echo "done"
